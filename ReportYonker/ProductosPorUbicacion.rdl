  <DataSets>
    <DataSet Name="Pro">
      <Query>
        <DataSourceName>Cantidad</DataSourceName>
        <CommandText>select count(p.productoID) as Cantidad_Productos, u.nombreU as Hubicacion
from Producto p
inner join Yonker y
on
p.yonkerID = y.yonkerID
inner join ubicacion u
on
y.ubicacionID = u.ubicacionID
group by u.nombreU</CommandText>
      </Query>
      <Fields>
        <Field Name="Cantidad_Productos">
