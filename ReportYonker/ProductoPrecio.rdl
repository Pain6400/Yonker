  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ProductoPrecio</DataSourceName>
        <CommandText>select nombrepro as Nombre, precio as Precio, a.nombreca as Categoria from Producto p
inner join Categoria a
on
p.categoriaID = a.categoriaID
where precio
BETWEEN 1000 and 3000 
and a.categoriaID = 1
order by precio asc</CommandText>
      </Query>
